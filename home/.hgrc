# example config (see "hg help config" for more info)

[ui]
# name and email, e.g.
# username = Jane Doe <jdoe@example.com>
username = kchiba <kchiba@winproxy.ne.jp>
merge = meld

[extensions]
hgext.convert =
extdiff =
color =
graphlog =
fetch =
rebase =
mq =
transplant =
record =
shelve =
info = ~/.mercurial/extensions/info.py
